{
  "name": "imgsrv",
  "version": "1.0.0",
  "description": "A proxy server which optimizes images",
  "main": "./server.js",
  "scripts": {
    "start": "node server.js",
    "debug": "nodemon --legacy-watch --watch ./ --inspect=0.0.0.0:9222 --nolazy ./server.js",
    "docker:debug": "docker-compose up",
    "docker:build": "docker build . -t imgsrv",
    "docker:run": "docker run --name imgsrv -p 56789:80 imgsrv",
    "docker:stop": "docker stop imgsrv & docker rm imgsrv",
    "docker:attach": "docker attach imgsrv",
    "docker": "npm run docker:stop && npm run docker:build && npm run docker:run"
  },
  "author": "laban@eilers.com",
  "license": "ISC",
  "dependencies": {
    "await-stream-ready": "^1.0.1",
    "express": "^4.16.3",
    "request": "^2.87.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "nodemon": "^1.17.5"
  }
}
